{
  "name": "Save | SaveAs | Cancel",
  "version": "1.0",
  "description": "A Chrome Extension which allows the User to choose whether to Save | SaveAs | Cancel any Download.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "permissions": ["tabs", "downloads", "downloads.shelf"],
  "icons": {
    "16": "images/download96.png",
    "32": "images/download96.png",
    "48": "images/download96.png",
    "128": "images/download96.png"
  },
  "manifest_version": 2
}